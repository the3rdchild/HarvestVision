K_min = 0.534513309  # Minimum multiplier (January)
K_max = 1.718665871  # Maximum multiplier (July)
P_min = 6.7          # Minimum harvest yield (tons)
P_max = 8.2          # Maximum harvest yield (tons)

# Calculating 'a' and 'b' for the linear equation H = a * K + b
a = (P_max - P_min) / (K_max - K_min)
b = P_min - a * K_min

a, b

